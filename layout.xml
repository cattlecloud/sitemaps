<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {{- range .}}
    <url>
      <loc>{{.Location}}</loc>
      <lastmod>{{.Modified.ISO}}</lastmod>
      <changefreq>{{.Frequency}}</changefreq>
      <priority>{{.Priority}}</priority>
    </url>
  {{- end}}
</urlset>
